<section class="section-card">
      <div class="container mt-4">
        <div class="row">

          {{#each products}}
          
            <div class="col-md-3">
              <div class="card" style="width: 18rem;">
                <img
                  class="card-img-top"
                  src="{{this.image}}"
                  alt="Card image cap"
                />
                <div class="card-body">
                  <h5 class="card-title">{{this.name}}</h5>
                  <p class="card-text">{{this.discreption}}</p>
                  <a href="#" class="btn btn-primary">Add to cart</a>
                </div>
              </div>
            </div>
          {{/each}}
        </div>

      </div>
    </section>
